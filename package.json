{
  "name": "custom-ssr-framework",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server/dev.ts",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "build": "npm run build:client && npm run build:server",
    "preview": "NODE_ENV=production node dist/server/main.js",
    "format:write": "prettier --write --parser typescript '**/*.{js,mjs,cjs,ts,tsx}'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.7",
    "express": "^5.1.0",
    "nprogress": "^0.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "6.30.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.2",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "esbuild-plugin-ignore": "^1.1.1",
    "prettier": "^3.6.2",
    "sass": "^1.94.0",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^6.3.3",
    "vite-plugin-mkcert": "^1.17.9"
  }
}
